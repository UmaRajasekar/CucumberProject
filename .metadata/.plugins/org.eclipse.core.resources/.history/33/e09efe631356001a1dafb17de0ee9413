package stepDefinitions;

import cucumber.api.PendingException;
import cucumber.api.java.en.Given;
import cucumber.api.java.en.When;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.And;
import cucumber.api.junit.Cucumber;
import org.junit.runner.RunWith;

@RunWith(Cucumber.class)
public class IndiaReport {

    @Given("^A browser is opened$")
    public void a_browser_is_opened() throws Throwable {
        throw new PendingException();
    }

    @When("^A India-Wiki URL is entered$")
    public void a_indiawiki_url_is_entered() throws Throwable {
        throw new PendingException();
    }

    @Then("^A web page opened with India displayed in block letters$")
    public void a_web_page_opened_with_india_displayed_in_block_letters() throws Throwable {
        throw new PendingException();
    }

    @And("^Article and Talk tabs are displayed$")
    public void article_and_talk_tabs_are_displayed() throws Throwable {
        throw new PendingException();
    }

}
